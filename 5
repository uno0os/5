#include <stdio.h>

int main() {
    int n;
    const int MOD = 12345;

    printf("Введіть довжину послідовностей (1 < n < 10000): ");
    scanf("%d", &n);
    
    if (n == 1) {
        printf("Кількість шуканих послідовностей: 2\n");
        return 0;
    }

    int a[n+1], b[n+1], c[n+1];
    a[1] = 1; // "0"
    b[1] = 1; // "1"
    c[1] = 0; // неможлива послідовність

    for (int i = 2; i <= n; i++) {
        a[i] = (a[i-1] + b[i-1] + c[i-1]) % MOD;
        b[i] = a[i-1] % MOD;
        c[i] = b[i-1] % MOD;
    }

    int result = (a[n] + b[n] + c[n]) % MOD;
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
